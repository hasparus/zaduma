{
  "name": "@zaduma/astro-starter",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.30.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "astro dev",
    "vercel:dev": "vercel dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "serve": "astro preview",
    "astro": "astro",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "ci": "pnpm lint && prettier --check src",
    "test": "playwright test"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^6.3.10",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/solid-js": "^5.1.3",
    "@astrojs/tailwind": "^6.0.2",
    "@fontsource-variable/brygada-1918": "^5.0.19",
    "@fontsource-variable/inter": "^5.0.18",
    "@hasparus/tailwind": "^1.1.7",
    "@mdx-js/mdx": "3.0.1",
    "@shikijs/twoslash": "^3.22.0",
    "@types/react": "^18.3.3",
    "@types/unist": "^3.0.2",
    "@vercel/og": "^0.6.2",
    "astro": "^5.17.2",
    "astro-eslint-parser": "^1.0.2",
    "eslint-plugin-markdown": "^5.0.0",
    "estree-util-value-to-estree": "^3.1.1",
    "plaiceholder": "^3.0.0",
    "postcss": "8.4.38",
    "reading-time": "^1.5.0",
    "remark-supersub": "^1.0.0",
    "sharp": "0.32.6",
    "shiki": "^3.22.0",
    "solid-js": "^1.8.17",
    "tailwindcss": "^3.4.3",
    "twoslash": "^0.3.6",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vercel": "^39.3.0",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@astrojs/rss": "^4.0.15",
    "@edgeandnode/eslint-config": "^2.0.3",
    "@playwright/test": "^1.58.2",
    "@types/hast": "3.0.4",
    "@types/node": "20.13.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.0",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "tsx": "^4.11.0",
    "typescript": "^5.8.3",
    "vite": "^5.2.12"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "rollup"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "core-js-pure",
      "puppeteer",
      "sharp"
    ]
  }
}
